; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Beanjot"
run/main_scene="res://Beanjot.tscn"
config/features=PackedStringArray("4.0", "GL Compatibility")
boot_splash/bg_color=Color(0.117647, 0.129412, 0.141176, 1)
boot_splash/image="res://Assets/Splash Screen.png"
config/icon="res://icon.svg"
config/windows_native_icon="res://Assets/icon.ico"

[display]

window/size/viewport_width=600
window/size/viewport_height=600

[internationalization]

locale/translation_remaps={}
locale/translations=PackedStringArray("res://Localization/Beanjot Localization.en.translation", "res://Localization/Beanjot Localization.es.translation")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
